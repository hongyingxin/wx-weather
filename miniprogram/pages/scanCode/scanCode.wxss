.test{
  width: 100rpx;
  height: 100rpx;
}
.swiper-box{
  width: 100%;
  height: 360rpx;
}
.swiper-child{
  width: 33.3%;
  height: 320rpx;
  display: inline-block;
  box-sizing: border-box;
  padding: 10rpx;
}
.swiper-img{
  width: 100%;
  height: 100%;
}
.book-box{
  width: 100%;
  height: 200rpx;
  box-sizing: border-box;
  display: flex;
  margin-bottom: 20rpx;
}
.book-left{
  width: 150rpx;
  height: 200rpx;
  padding-left: 20rpx;
  padding-right: 30rpx;
}
.book-left-img{
  width: 100%;
  height: 100%;
}
.book-conter{
  width: 350rpx;
  height: 100%;
}
.book-title-one{
  margin-bottom: 5rpx;
  color: #000;
  font-size: 30rpx;
}
.book-title-two{
  margin-bottom: 4rpx;
  color: #666;
  font-size: 28rpx;
}
.book-right{
  width: 200rpx;
}
.rating{
  height: 50rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #8e8e8e;
}
.rating-box{
  width: 100%;
  display: flex;
  font-size: 26rpx;
  color: #3e3e3e;
  align-items: center;
}
.empty-rat{
  padding-left: 20rpx;
}
.all-rat{
  color: red;
  font-size: 36rpx;
  padding-left: 20rpx;
}
.num-rat{
  font-size: 24rpx;
  color: #8e8e8e;
}